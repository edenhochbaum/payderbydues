		<h1>Fee Schedule Admin</h1>
		<div class="row">
		<div class="col-xs-12">
                <form method="post">
                        <div class="form-group">
                                <label for="feeschedulename">Name</label>
                                <input type="text" class="form-control" id="feeschedulename" name="name" placeholder="name of the fee schedule" required>
                        </div>
                        <div class="form-group">
                                <label for="feeschedulevalue">Value</label>
                                <input type="number" class="form-control" id="costofthefeeschedule" name="value" placeholder="$" required>
                        </div>
						<input type="hidden" value="Add" name="nextoperation">
						<button type="submit" class="btn btn-primary">Submit</button>
                </form>
		</div>
		</div> <!-- /row -->
	<br><br>
		<div class="row">
		<div class="col-xs-12">
		  <table id="example" class="datatable display">
			<thead>
				<tr>
					<!-- <th>ID</th> -->
					<th>Name</th>
					<!-- <th>League ID</th> -->
					<th>Value</th>
				</tr>
			</thead>

			<tbody>
				{{#each rows}}
				<tr>
					<!-- <td>{{id}}</td> -->
					<td>{{name}}</td>
					<!-- <td>{{leagueid}}</td> -->
					<td>{{value}}</td>
				 </tr>
				{{/each}}
			</tbody>
		  </table>
			</div>
		</div> <!-- /row -->
